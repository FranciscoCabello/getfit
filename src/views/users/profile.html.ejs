<div class="content-area">
    <div class="content-section-area">
        <h1 class="section-header"><svg aria-hidden="true" width="30" height="30" focusable="false" data-prefix="fas" data-icon="cog" class="svg-inline--fa fa-cog fa-w-16" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 512 512">
            <path fill="currentColor" d="M487.4 315.7l-42.6-24.6c4.3-23.2 4.3-47 0-70.2l42.6-24.6c4.9-2.8 7.1-8.6 5.5-14-11.1-35.6-30-67.8-54.7-94.6-3.8-4.1-10-5.1-14.8-2.3L380.8 110c-17.9-15.4-38.5-27.3-60.8-35.1V25.8c0-5.6-3.9-10.5-9.4-11.7-36.7-8.2-74.3-7.8-109.2 0-5.5 1.2-9.4 6.1-9.4 11.7V75c-22.2 7.9-42.8 19.8-60.8 35.1L88.7 85.5c-4.9-2.8-11-1.9-14.8 2.3-24.7 26.7-43.6 58.9-54.7 94.6-1.7 5.4.6 11.2 5.5 14L67.3 221c-4.3 23.2-4.3 47 0 70.2l-42.6 24.6c-4.9 2.8-7.1 8.6-5.5 14 11.1 35.6 30 67.8 54.7 94.6 3.8 4.1 10 5.1 14.8 2.3l42.6-24.6c17.9 15.4 38.5 27.3 60.8 35.1v49.2c0 5.6 3.9 10.5 9.4 11.7 36.7 8.2 74.3 7.8 109.2 0 5.5-1.2 9.4-6.1 9.4-11.7v-49.2c22.2-7.9 42.8-19.8 60.8-35.1l42.6 24.6c4.9 2.8 11 1.9 14.8-2.3 24.7-26.7 43.6-58.9 54.7-94.6 1.5-5.5-.7-11.3-5.6-14.1zM256 336c-44.1 0-80-35.9-80-80s35.9-80 80-80 80 35.9 80 80-35.9 80-80 80z"></path>
        </svg> Configuracion perfil:</h1>
        <div class="items-table">
            <h1 class="items-table">Personal</h1>
            <div class="display-administrable">
                <div class="form-div">
                    <img src="<%= user.photo %>" width="250" height="300"/>
                </div>
                <div class="form-main">
                    <div class="form-div">
                        <form class="form" >
                            <p>Datos:</p>
                            <input name="name" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" value="<%= user.name %>" id="name" readonly/>
                            <input name="name" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" value="<%= user.lastname %>" id="name" readonly/>
                            <input name="name" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" value="<%= user.email %>" id="name" readonly/>
                            <input name="name" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" value="<%= user.phone %>" id="name" readonly/>
                            <a href='<%= pathForm %>' class="btn-custom-act-profile">Actualizar perfil</a>
                        </form>
                    </div>
                </div>
            </div>
        </div>
        <div class="items-table">
            <h1 class="items-table">Informacion bancaria</h1>
            <div class="display-administrable">
                <div class="form-div">
                    <form class="form" action="<%= deleteBankAccountPath %>" >
                        <% if (bankAccount) { %>
                            Nombre titular:
                            <input name="name" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" value="<%= bankAccount.name %>" id="name" readonly/>
                            Rut titular:
                            <input name="name" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" value="<%= bankAccount.rut %>" id="name" readonly/>
                            Cuenta:
                            <input name="name" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" value="<%= bankAccount.account %>" id="name" readonly/>
                            Tipo cuenta:
                            <input name="name" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" value="<%= bankAccount.type %>" id="name" readonly/>
                            Banco:
                            <input name="name" type="text" class="validate[required,custom[onlyLetter],length[0,100]] feedback-input" value="<%= bankAccount.bank %>" id="name" readonly/>
                        <% } else { %>
                            <a href="<%= createBankAccountPath %>" class="btn-custom-create-bank" >Agregar cuenta bancaria</a>
                        <% } %>
                    </form>
                </div>
            </div>
            <% if (bankAccount) { %>
            <form method='post' action="<%= deleteBankAccountPath %>"><input type='submit' value="Eliminar cuenta" class="btn-custom-delete-bank" /></form>
            <% } %>
        </div>
    </div>
    <div class="content-section-area">
        <h1 class="section-header"> <svg aria-hidden="true" width="30" height="30" focusable="false" data-prefix="fas" data-icon="user" class="svg-inline--fa fa-user fa-w-14" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512">
            <path fill="currentColor" d="M224 256c70.7 0 128-57.3 128-128S294.7 0 224 0 96 57.3 96 128s57.3 128 128 128zm89.6 32h-16.7c-22.2 10.2-46.9 16-72.9 16s-50.6-5.8-72.9-16h-16.7C60.2 288 0 348.2 0 422.4V464c0 26.5 21.5 48 48 48h352c26.5 0 48-21.5 48-48v-41.6c0-74.2-60.2-134.4-134.4-134.4z"></path>
        </svg> Admnistracion cuenta:</h1>
        <div class="items-table">
            <h1 class="items-table">Inscripciones</h1>
            <div class="display-table">
                <a href="<%= subscriptionLocalPath %>" class="btn-custom-local">Locales</a>
                <a href="<%= subscriptionActivitiesPath %>" class="btn-custom-act">Actividades</a>
            </div>
        </div>
        <div class="items-table">
            <h1 class="items-table">Administrable</h1>
            <% ownedLocals.forEach((local) => { %>
                <div class="display-table">
                    <div>
                        <p>
                            <%= local.name; %>
                        </p>
                    </div>
                    <div>
                        <a href="<%=  viewLocalOwnerPath(local.id) %>" class="btn-custom-admin">Administrar</a>
                    </div>
                </div>
            <% }); %>    
        </div>
        <div class="items-table">
            <h1 class="items-table">Tienda</h1>
            <div class="tbl-header">
                <table cellpadding="0" cellspacing="0" border="0">
                    <thead>
                        <tr>
                            <th><p>Articulo</p></th>
                            <th><p>Precio</p></th>
                            <th><p>Tipo</p></th>
                            <th><p>Estado</p></th>
                            <th><p></p></th>
                        </tr>
                    </thead>
                </table>
            </div>
            <div class="tbl-content">
                <table cellpadding="0" cellspacing="0" border="0">
                    <tbody>
                        <% products.forEach((product) => { %>
                            <tr>
                                <td><p><%= product.name %></p></td>
                                <td><p>$<%= product.precio %></p></td>
                                <td><p><%= product.tipo %></p></td>
                                <td><p><%= product.estado %></p></td>
                                <td><form method="post" action="<%= destroyProductPath(product.id) %>"><input type='submit' value='Eliminar' class="btn-custom-delete"/></form></td>
                            </tr>
                        <% }); %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>